#include<stdlib.h>
#include<time.h>
/**
 *main - entry point
 *
 *Return: Always 0 (Success)
 *
 */
int main(void)
{
	int n, lastDigit;
	lastDigit = n % 10;
	
	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (lastDigit > 5 );
	{
		printf("Last digit of %d is lastDigit and is
                                     less tha 6 and not o\n", n);
	}
	else if (lastDigit==0);
	{
		printf("Last digit of %d is lastDigit and is 0");
	}
	else
	{
		printf("Last digit of %d is lastdigit and i
                                    s less than 6 and not 0");
	}
	return (0)
}
