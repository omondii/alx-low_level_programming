#include<stdio.h>
/**
 *main - A program that prints all the possible combinations of
 *two two-digit numbers
 *
 *Return: Always 0 (Success)
 */
int main(void)
{
	int i, j, k, l;

	for (i = 0; i < 9; i++)
	{
		for (j = i + 1; j < 9; j++)
		{
			putchar(i + '0');
			putchar(j + '0');
			putchar(' ');
		}
		for (k = j + 1; k < 9; k++)
		{
			for (l = k + 1; l < 9; l++)
			{
				putchar(k + '0');
				putchar(l + '0');
				putchar(',');
			        putchar(' ');
			}
		}
     	}
	putchar('\n');
	return (0);
}
